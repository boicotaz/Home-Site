{
    "plugins": ["plugins/markdown"],
    
    "source": {
        "include": [ "controllers"],
        "exclude": "(node_modules/|docs)",
        "includePattern": ".+\\.js(doc|x)?$"
    },
    "opts":{
        "recurse":true,
        "destination":"./jsdoc/docs",
        "template": "./jsdoc/template"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
}